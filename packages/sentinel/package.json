        "start": "node --enable-source-maps dist/server.js"
    },
    "dependencies": {
        "dotenv": "^17.2.3",
        "fastify": "^5.6.2",
        "jsonwebtoken": "^9.0.2",
        "lodash-es": "^4.17.21",
        "pino": "^10.1.0",
        "pino-pretty": "^13.1.2"
    },
    "engines": {
        "node": ">=18"
    }
}