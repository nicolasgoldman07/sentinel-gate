{
    "name": "sentinel",
    "version": "1.0.0",
    "private": true,
    "main": "dist/server.js",
    "type": "module",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc -p tsconfig.build.json",
