{
    "name": "sentinel",
    "version": "1.0.0",
    "private": true,
    "main": "dist/server.js",
    "type": "module",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc -p tsconfig.build.json",
        "typecheck": "tsc -p tsconfig.json --noEmit --allowImportingTsExtensions",
        "start": "node --enable-source-maps dist/server.js"
    },
    "dependencies": {
        "@aws-sdk/client-dynamodb": "^3.927.0",
        "@aws-sdk/util-dynamodb": "^3.927.0",
        "dotenv": "^17.2.3",
        "fastify": "^5.6.2",
        "json-logic-js": "^2.0.5",
        "jsonwebtoken": "^9.0.2",
        "jwks-rsa": "^3.1.0",
        "lodash-es": "^4.17.21",
        "pino": "^10.1.0",
        "pino-pretty": "^13.1.2"
    },
    "devDependencies": {
        "@types/json-logic-js": "^2.0.8",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^24.10.0",
        "@types/pino": "^7.0.4",
        "dynamodb-local": "^0.0.36",
        "prettier": "^2.8.8",
        "ts-node": "^10.9.2",
        "tsx": "^4.12.0",
        "typescript": "^5.9.3"
